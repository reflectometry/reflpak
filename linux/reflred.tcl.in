#! /bin/sh
# \
exec "@WISH@" "$0" "$@"

set VIEWRUN_HOME "@VIEWRUN_HOME@"
set TKCON "@TCL_PREFIX@/lib/tkcon.tcl"
set OCTAVE_HOST localhost:1515

## ==============================================================
## Installation dependencies
## ==============================================================

## You may need to customize this file for each installation.  It
## defines the locations of the various programs on the system.  There
## are no user dependencies in this file.

if { [namespace exists ::freewrap] } {
    source [file join $VIEWRUN_HOME loadwrap.tcl]
    set dir "@TCL_PREFIX@/lib/bwidget1.6"
    source $dir/pkgIndex.tcl
    #set dir "@TCL_PREFIX@/lib/tkdnd1.0"
    #source $dir/pkgIndex.tcl
    set dir "@TCL_PREFIX@/lib/Tktable2.8"
    source $dir/pkgIndex.tcl
    set dir "@TCL_PREFIX@/lib/octave0.1"
    source $dir/pkgIndex.tcl
}

package require BWidget
#package require tkdnd
package require Tktable
package require BLT
package require octave

source [file join $VIEWRUN_HOME viewrun.tcl]
